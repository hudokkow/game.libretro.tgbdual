<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.tgbdual"
		name="GBC (tgbdual)"
		version="1.0.1"
		provider-name="Libretro">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
                   library_android="game.libretro.tgbdual.so"
                   library_linux="game.libretro.tgbdual.so"
                   library_osx="game.libretro.tgbdual.dylib"
                   library_windx="game.libretro.tgbdual.dll"
                   library_wingl="game.libretro.tgbdual.dll">
		<platforms>gb|gbc</platforms>
		<extensions>gb|gbc</extensions>
		<supports_vfs>true</supports_vfs>
		<supports_no_game>false</supports_no_game>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">tgbdual v1.0.1</summary>
		<description>Source: https://github.com/libretro/tgbdual-libretro</description>
		<platform>all</platform>
	</extension>
</addon>
